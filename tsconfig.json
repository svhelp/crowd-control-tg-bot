{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "rootDir": "src",
    "outDir": "api",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
}
  